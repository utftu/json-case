{
  "name": "json-case",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "rollup -c",
    "format": "prettier --write \"./**/*.{js,mjs,cjs,md,json,ts}\"",
    "types": "tsc -p tsconfig.types.json",
    "publish": "npm run format && npm run build && npm run types && npm run test && lerna publish"
  },
  "types": "./dist/types/index.d.ts",
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "exports": {
    "development": {
      "import": "./dist/esm/dev.mjs",
      "require": "./dist/cjs/dev.js"
    },
    "producton": {
      "import": "./dist/esm/prod.mjs",
      "require": "./dist/cjs/prod.js"
    },
    "import": "./dist/esm/prod.mjs",
    "require": "./dist/cjs/prod.js",
    "default": "./dist/cjs/prod.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "author": "",
  "license": "ISC",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/jest": "^26.0.23",
    "babel-jest": "^26.6.3",
    "lerna": "^4.0.0",
    "prettier": "^2.2.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.2.4"
  }
}
